{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "strict": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "types": [
      "@pinia/nuxt",
      "vite/client",
    ]
  }
}
